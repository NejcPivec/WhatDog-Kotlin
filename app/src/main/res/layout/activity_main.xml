<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/ozadje"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" android:background="@drawable/ic_ozadje">


    <AutoCompleteTextView
            android:layout_width="207dp"
            android:layout_height="28dp"
            android:inputType="textPersonName"
            android:background="@drawable/ic_search_input"
            android:hint="Wikipedia dog search"
            android:paddingLeft="10dp"
            android:ems="10"
            android:id="@+id/search"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.061"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/imageView3"
            android:popupBackground="@drawable/ic_search_input"/>

    <TextView

            android:background="@drawable/whatdog"
            android:layout_width="179dp"
            android:layout_height="67dp"
            android:id="@+id/textView"
            android:layout_marginTop="160dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"/>


    <ImageView
            android:layout_width="217dp"
            android:layout_height="237dp" app:srcCompat="@drawable/main_button"
            android:id="@+id/glavni_gumb"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.528"
            android:layout_marginTop="40dp" app:layout_constraintTop_toBottomOf="@+id/textView"/>
    <ImageView
            android:layout_width="61dp"
            android:layout_height="78dp" app:srcCompat="@drawable/ic_past_seaches_icon"
            android:id="@+id/imageView2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintVertical_bias="0.032" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/search" app:layout_constraintHorizontal_bias="0.0"
            tools:srcCompat="@drawable/ic_past_seaches_icon"/>


    <ImageView
            android:layout_width="43dp"
            android:layout_height="45dp"
            android:id="@+id/imageView3" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.976"
            app:layout_constraintVertical_bias="0.04"
            android:onClick="sendMessage" tools:srcCompat="@drawable/ic_search_icon"
            app:srcCompat="@drawable/ic_search_icon"/>


    <TextView
            android:background="@drawable/test"
            android:layout_width="269dp"
            android:layout_height="42dp"
            android:id="@+id/textView2" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.682" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/glavni_gumb" app:layout_constraintVertical_bias="0.094"/>
    <ImageView
            android:layout_width="78dp"
            android:layout_height="74dp"
            android:background="@drawable/puscica_levo"
            android:id="@+id/imageView4"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.209"/>


</android.support.constraint.ConstraintLayout>